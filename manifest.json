{
  "name": "ErrorsSpy",
  "description": "Track console & network issues and download their report files.",
  "version": "0.1",
  "permissions": [
    "debugger","alarms","tabs", "http://*/*", "https://*/*","*://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon.green.png",
    "default_title": "ErrorsSpy"
  },
  "manifest_version": 2
}